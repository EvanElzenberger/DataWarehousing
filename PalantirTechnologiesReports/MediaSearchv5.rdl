  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>PalantirTech</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ArrestType">
            <Value>=Parameters!ArrestType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        SubjectIdentity.FirstName, SubjectIdentity.LastName, MediaType.MediaType, SubjectSocialMedia.MediaContent, SubjectSocialMedia.SecurityThreatLevel
FROM            SubjectSocialMedia INNER JOIN
                         SubjectIdentity ON SubjectSocialMedia.SubjectID = SubjectIdentity.SubjectID INNER JOIN
                         MediaType ON SubjectSocialMedia.MediaTypeID = MediaType.MediaTypeID INNER JOIN
                         SubjectArrestRecord ON SubjectIdentity.SubjectID = SubjectArrestRecord.SubjectID INNER JOIN
                         ArrestType ON SubjectArrestRecord.ArrestTypeID = ArrestType.ArrestTypeID
WHERE        (SubjectArrestRecord.ArrestTypeID = @ArrestType)
ORDER BY SubjectIdentity.FirstName, SubjectIdentity.LastName</CommandText>
      </Query>
      <Fields>
        <Field Name="FirstName">
          <DataField>FirstName</DataField>
